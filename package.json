  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "list:tests": "lab --assert @hapi/code --dry --verbose",
    "test": "npm run build && npm run lint && npm run test:run",
